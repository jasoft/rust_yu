{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Rust Yu",
  "identifier": "com.rust-yu.app",
  "version": "0.1.0",
  "build": {
    "devUrl": "http://localhost:11000",
    "beforeDevCommand": "cd src-frontends/webui && npm run dev",
    "beforeBuildCommand": "cd src-frontends/webui && npm run build",
    "frontendDist": "./src-frontends/webui/dist"
  },
  "app": {
    "windows": [
      {
        "title": "Rust Yu - Windows Uninstaller",
        "width": 1000,
        "height": 700,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  }
}
